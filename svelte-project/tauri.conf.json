{
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../public",
    "distDir": "../dist"
  },
  "package": {
    "productName": "svelte-project",
    "version": "1.0.0"
  },
  "tauri": {
    "windows": [
      {
        "title": "Svelte Project",
        "width": 800,
        "height": 600,
        "resizable": true
      }
    ],
    "bundle": {
      "active": true,
      "targets": ["msi"],
      "identifier": "com.example.svelteproject",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png"
      ],
      "resources": [],
      "externalBin": [],
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "10.13",
        "license": "MIT"
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "http://timestamp.digicert.com"
      }
    }
  }
}
